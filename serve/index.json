{
    "@odata.type": "#ServiceRoot.v1_0_2.ServiceRoot",
    "@odata.context": "/redfish/v1/$metadata#ServiceRoot",
    "Id": "RootService",
    "Name": "Root Service",
    "RedfishVersion": "1.0.2",
    "UUID": " {{- index . "root_UUID" -}} ",
{{ range $index, $value := index . "root_links" }}
    "{{- $value.Name -}}": {
        "odata.id": "{{- $value.Target -}}"
    },{{end}}
    "Links": {
        "Sessions": {
            "@odata.id": "/redfish/v1/SessionService/Sessions"
        }
    },
    "Oem": {},
    {{ index . "odata_self_id" }},
    {{ index . "redfish_std_copyright"}}
}

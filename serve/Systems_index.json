{
    "@odata.type": "#ComputerSystemCollection.ComputerSystemCollection",
    "Name": "Computer System Collection",
    "Members@odata.count": {{index . "systems" | len}},
    "Members": [
{{ range $index, $value := index . "systemList" }}
    {{ if $index }},{{end}}
    {
        "@odata.id": "/redfish/v1/Systems/{{- $value -}}"
    }{{end}}

    ],
    "@odata.context": "/redfish/v1/$metadata#Systems",
    {{ index . "odata_self_id" }},
    {{ index . "redfish_std_copyright"}}
}
